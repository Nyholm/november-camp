{% extends "SymfonyseEventBundle::base.html.twig" %}

{% block title %}
Event — {{parent()}}
{% endblock %}

{% block content %}

    Detta är våra event

    <ul>
        {% for event in events %}
            <li>
                <a href="{{ path('event', {permalink:event.permalink}) }}">{{ event.title }}</a>
            </li>
        {% endfor %}
    </ul>

{% endblock %}
